{
  "name": "remove-duplicate-emails",
  "version": "1.0.1",
  "description": "A node module that removes +1 trick at the end of emails on sign up. To stop duplicate accounts.",
  "homepage": "",
  "author": {
    "name": "Robert James Gabriel",
    "email": "robert_gabriel@outlook.com",
    "url": "https://www.robertgabriel.ninja"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "gdpr",
    "emails",
    "signup",
    "tinyurl",
    "short"
  ],
  "devDependencies": {
    "nsp": "^2.8.0",
    "eslint": "^4.19.1",
    "prettier": "^1.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-config-xo": "^0.20.1",
    "jest": "^22.0.6"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "RobertJGabriel/remove-duplicate-emails",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT"
}
